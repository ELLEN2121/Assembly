INICIO: 

	MOV A, 15	
	MOV B, 2
	CMP B,A               ; B =<A ? 
	JBE CALCULO 
	JE FIM
		
		
CALCULO: 
	DIV B ; ARMAZENA EM A = A/B 
CALCULO_1: 
	CMP B,A
	JA CALCULO_2 
	JE FIM 

CALCULO_2:
	ADD B, 1
	CMP B,A
	JBE CALCULO_2 ; RESULTADO DE CALCULO 2 Ã‰ B>A
	JE FIM 
FIM:
  	HLT
	

; INSERIR A = 10 E B= 2, SE B=<A, IR PARA CALCULO. 
; DIVIDIR A = A/B 
; B>A, SE SIM, IR PARA CALCULO 2
